{
  "name": "nestjs-asyncapi",
  "version": "0.0.1",
  "description": "Async API module for Nestjs",
  "author": "Rodrigo",
  "license": "MIT",
  "main": "dist/index",
  "types": "dist/index",
  "engines": {
    "node": ">= 10.13.0",
    "npm": ">= 6.11.0"
  },
  "contributors": [
    {
      "name": "Rodrigo",
      "author": true
    },
    {
      "name": "Vlad Betsun",
      "email": "vlad.betcun@gmail.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/underfisk/nestjs-asyncapi/issues"
  },
  "homepage": "https://github.com/underfisk/nestjs-asyncapi#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config e2e/jest-e2e.json"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/core": "^8.2.6",
    "reflect-metadata": "^0.1.13"
  },
  "dependencies": {
    "@asyncapi/generator": "^1.8.27",
    "@asyncapi/html-template": "^0.24.6",
    "@nestjs/mapped-types": "^1.0.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/common": "8.2.6",
    "@nestjs/core": "8.2.6",
    "@nestjs/platform-express": "8.2.6",
    "@nestjs/platform-fastify": "8.2.6",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/testing": "^8.2.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^3.12.7",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
